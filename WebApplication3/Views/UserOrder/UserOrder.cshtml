@model List<WebApplication3.ModelForView.ShoppingCartInfo>

<div style="width:80% ;margin:auto">
    @if (Model != null && Model.Count > 0)
    {
        <h1><b>My Order</b></h1>
        <table class="table table-striped table-bordered">

            <tr>
              
                <th>Created date</th>
                <th>Booking</th>
                
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                   
                    <td>@item.orders.CreatedDate</td>
                    
                    
                    <td>@item.orders.OrderStatus.StatusName</td> 
                </tr>
            }
        </table>

    }
    else
    {
        <h5>No order</h5>
    }
</div>
